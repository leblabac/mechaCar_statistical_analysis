Suspension_Coil <- read.csv('Suspension_Coil.csv', check.names = F, stringsAsFactors = F)
View(Suspension_Coil)
total_summary <- Suspension_Coil %>% summarize(Mean=mean(PSI),
Median=median(PSI),
Variance=var(PSI),
SD=sd(PSI),
.groups = 'keep')
View(total_summary)
